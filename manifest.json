{
  "manifest_version": 3,
  "name": "Automatemp",
  "version": "2.0",
  "icons": {
    "16": "images/logo16.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.wildberries.ru/*",
        "*://*.wb.ru/*",
        "*://62.109.3.23/*"
      ],
      "js": [
        "script.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "background": {
    "service_worker": "bg.js"
  }
}